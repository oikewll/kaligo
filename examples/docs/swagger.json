                    {
                        "type": "string",
                        "description": "CSRF Token",
                        "name": "csrf",
                        "in": "formData",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "type": "string"
                        }
                    }
                }
            }
